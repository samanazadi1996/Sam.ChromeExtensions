{
    "manifest_version": 2,
    "name": "Auto LinkedIn",
    "version": "1.0",
    "description": "Auto LinkedIn",
    "permissions": [
        "storage",
        "declarativeContent",
        "activeTab",
        "scripting"
    ],
    "browser_action": {
        "default_popup": "main.html",
        "default_icon": {
            "16": "images/icon16.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}